<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.dell.univstarproject.sign.activity.SetActivity">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp">
        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/set_back"
            android:src="@mipmap/register_return"
            android:background="#00000000"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="12dp"
            android:layout_marginStart="12dp" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="设置"
            android:textSize="18dp"
            android:textColor="#0c66f6"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true" />
    </RelativeLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:background="#ddd8d8"/>
    <RelativeLayout
        android:id="@+id/set_jiebang"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="#ffff">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:text="换绑手机"
            android:gravity="center"
            android:layout_marginLeft="10dp"
            android:textColor="#0f0f0f"
            />
        <TextView
            android:id="@+id/shoujihao"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="18835794656"
            android:layout_alignTop="@+id/imageView17"
            android:layout_toLeftOf="@+id/imageView17"
            android:layout_toStartOf="@+id/imageView17"
            android:layout_marginRight="12dp"
            android:layout_marginEnd="12dp" />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/home_myself_right_arrow"
            android:layout_marginRight="10dp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="10dp"
            android:id="@+id/imageView17" />
    </RelativeLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#dad7d7"/>
    <RelativeLayout
        android:id="@+id/set_shejiao"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="#ffff">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:text="社交账号绑定"
            android:gravity="center"
            android:layout_marginLeft="10dp"
            android:textColor="#0f0f0f"
            />
        <TextView
            android:id="@+id/shejiao"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="绑定/解绑"
            android:layout_alignTop="@+id/imageView18"
            android:layout_toLeftOf="@+id/imageView18"
            android:layout_toStartOf="@+id/imageView18"
            android:layout_marginRight="12dp"
            android:layout_marginEnd="12dp" />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/home_myself_right_arrow"
            android:layout_marginRight="10dp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="10dp"
            android:id="@+id/imageView18" />
    </RelativeLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#dad7d7"/>
    <RelativeLayout
        android:id="@+id/set_mima"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="#ffff">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:text="登录密码"
            android:gravity="center"
            android:layout_marginLeft="10dp"
            android:textColor="#0f0f0f"
            />
        <TextView
            android:id="@+id/mima"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="修改"
            android:layout_alignTop="@+id/imageView19"
            android:layout_toLeftOf="@+id/imageView19"
            android:layout_toStartOf="@+id/imageView19"
            android:layout_marginRight="12dp"
            android:layout_marginEnd="12dp" />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/home_myself_right_arrow"
            android:layout_marginRight="10dp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="10dp"
            android:id="@+id/imageView19"/>
    </RelativeLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:background="#dad7d7"/>
    <RelativeLayout
        android:id="@+id/set_qingchu"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="#ffff">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:text="清除缓存"
            android:gravity="center"
            android:layout_marginLeft="10dp"
            android:textColor="#0f0f0f"
            />
        <TextView
            android:id="@+id/huancun"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#0f94f4"
            android:text="huancun"
            android:layout_alignTop="@+id/imageView20"
            android:layout_toLeftOf="@+id/imagebuttonaa"
            android:layout_toStartOf="@+id/imagebuttonaa" />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/home_myself_right_arrow"
            android:layout_marginRight="10dp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="10dp"
            android:id="@+id/imageView20" />
        <ImageButton
            android:id="@+id/imagebuttonaa"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/personal_delete_img"
            android:background="#00000000"
            android:layout_alignParentBottom="true"
            android:layout_toLeftOf="@+id/imageView20"
            android:layout_toStartOf="@+id/imageView20" />
    </RelativeLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:background="#dad7d7"/>
    <RelativeLayout
        android:id="@+id/set_shenhji"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="#ffff">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:text="关于UnivStar"
            android:gravity="center"
            android:layout_marginLeft="10dp"
            android:textColor="#0f0f0f"
            />
        <TextView
            android:id="@+id/shengji"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:layout_alignTop="@+id/imageView21"
            android:layout_toLeftOf="@+id/imageView21"
            android:layout_toStartOf="@+id/imageView21"
            android:layout_marginRight="12dp"
            android:layout_marginEnd="12dp" />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/home_myself_right_arrow"
            android:layout_marginRight="10dp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="10dp"
            android:id="@+id/imageView21" />
    </RelativeLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#dad7d7"/>
    <RelativeLayout
        android:id="@+id/set_tuichu"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="#ffff">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:text="退出登录"
            android:gravity="center"
            android:layout_marginLeft="10dp"
            android:textColor="#0f0f0f"
            />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/home_myself_right_arrow"
            android:layout_marginRight="10dp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="10dp"
            />
    </RelativeLayout>

</LinearLayout>
