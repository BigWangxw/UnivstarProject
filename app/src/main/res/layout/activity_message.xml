<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#ffff"
    tools:context="com.example.dell.univstarproject.sign.message.MessageActivity">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp">
        <ImageButton
            android:id="@+id/message_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/register_return"
            android:background="#00000000"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="13dp"
            android:layout_marginStart="13dp" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="消息提醒"
            android:textColor="#0992ed"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true" />
    </RelativeLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#e2dddd"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:id="@+id/message_dingdan">
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/message_order"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="13dp"
            android:layout_marginStart="13dp"
            android:id="@+id/imageView23" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="我的订单"
            android:textColor="#0b0b0b"
            android:layout_alignTop="@+id/imageView23"
            android:layout_toRightOf="@+id/imageView23"
            android:layout_toEndOf="@+id/imageView23"
            android:layout_marginLeft="14dp"
            android:layout_marginStart="14dp"
            android:id="@+id/textView8" />
        <TextView
            android:id="@+id/text_dingdan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="暂无提醒"
            android:textSize="10dp"
            android:layout_below="@+id/textView8"
            android:layout_alignLeft="@+id/textView8"
            android:layout_alignStart="@+id/textView8"/>
    </RelativeLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#d3cfcf"
        />
    <RelativeLayout
        android:id="@+id/message_zan"
        android:layout_width="match_parent"
        android:layout_height="80dp">
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/message_praise"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="14dp"
            android:layout_marginStart="14dp"
            android:id="@+id/imageView24" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="赞我的"
            android:textColor="#0f0f0f"
            android:layout_marginLeft="14dp"
            android:layout_marginStart="14dp"
            android:layout_alignTop="@+id/imageView24"
            android:layout_toRightOf="@+id/imageView24"
            android:layout_toEndOf="@+id/imageView24"
            android:id="@+id/text2"/>
        <TextView
            android:id="@+id/text_zan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="暂无消息"
            android:layout_below="@+id/text2"
            android:layout_alignLeft="@+id/text2"
            android:layout_alignStart="@+id/text2" />
    </RelativeLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#e1dede"/>
    <RelativeLayout
        android:id="@+id/message_works"
        android:layout_width="match_parent"
        android:layout_height="80dp">
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/message_comment"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="14dp"
            android:layout_marginStart="14dp"
            android:id="@+id/imageView26"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="评论我的"
            android:textColor="#0f0f0f"
            android:layout_marginLeft="14dp"
            android:layout_marginStart="14dp"
            android:layout_alignTop="@+id/imageView26"
            android:layout_toRightOf="@+id/imageView26"
            android:layout_toEndOf="@+id/imageView26"
            android:id="@+id/text3"/>
        <TextView
            android:id="@+id/text_ping"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="暂无消息"
            android:layout_below="@+id/text3"
            android:layout_alignLeft="@+id/text3"
            android:layout_alignStart="@+id/text3"/>
    </RelativeLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#e1dede"/>
    <RelativeLayout
        android:id="@+id/message_pinglun"
        android:layout_width="match_parent"
        android:layout_height="80dp">
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/message_job"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="14dp"
            android:layout_marginStart="14dp"
            android:id="@+id/imageView25"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="作业提醒"
            android:textColor="#0f0f0f"
            android:layout_marginLeft="14dp"
            android:layout_marginStart="14dp"
            android:layout_alignTop="@+id/imageView25"
            android:layout_toRightOf="@+id/imageView25"
            android:layout_toEndOf="@+id/imageView25"
            android:id="@+id/text5"/>
        <TextView
            android:id="@+id/text_works"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="暂无消息"
            android:layout_below="@+id/text5"
            android:layout_alignLeft="@+id/text5"
            android:layout_alignStart="@+id/text5" />
    </RelativeLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#e1dede"/>
    <RelativeLayout
        android:id="@+id/message_tuan"
        android:layout_width="match_parent"
        android:layout_height="80dp">
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/message_official"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="14dp"
            android:layout_marginStart="14dp"
            android:id="@+id/imageView27"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="UnivStar团队"
            android:textColor="#0f0f0f"
            android:layout_marginLeft="14dp"
            android:layout_marginStart="14dp"
            android:layout_alignTop="@+id/imageView27"
            android:layout_toRightOf="@+id/imageView27"
            android:layout_toEndOf="@+id/imageView27"
            android:id="@+id/text6"/>
        <TextView
            android:id="@+id/text_tuandui"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="暂无消息"
            android:layout_below="@+id/text6"
            android:layout_alignLeft="@+id/text6"
            android:layout_alignStart="@+id/text6" />
    </RelativeLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#e1dede"/>
    <RelativeLayout
        android:id="@+id/message_guanzhu"
        android:layout_width="match_parent"
        android:layout_height="80dp">
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/message_official"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="14dp"
            android:layout_marginStart="14dp"
            android:id="@+id/imageView28"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="关注提醒"
            android:textColor="#0f0f0f"
            android:layout_marginLeft="14dp"
            android:layout_marginStart="14dp"
            android:layout_alignTop="@+id/imageView28"
            android:layout_toRightOf="@+id/imageView28"
            android:layout_toEndOf="@+id/imageView28"
            android:id="@+id/text1"/>
        <TextView
            android:id="@+id/text_guanzhu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="暂无消息"
            android:layout_below="@+id/text1"
            android:layout_alignLeft="@+id/text1"
            android:layout_alignStart="@+id/text1" />
    </RelativeLayout>


</LinearLayout>
